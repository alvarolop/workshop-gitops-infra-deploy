#!/bin/bash
# CHECK VARIABLE VALUES ARE BETWEEN DOUBLE QUOTES!

#
# AWS credentials
# 

# AWS Region. Change according to your location
export AWS_DEFAULT_REGION=""
## AWS Access Key ID
export AWS_ACCESS_KEY_ID=""
## AWS Secret Access Key
export AWS_SECRET_ACCESS_KEY=""

#
# Cluster variables
# 

# Set "stable" if you want to install the latest stable version
# https://mirror.openshift.com/pub/openshift-v4/clients/ocp/
export OPENSHIFT_VERSION="4.17.20"

# Copy directly from demo.redhat.com
export BASE_DOMAIN=""

# Number of users for the workshop
export KEYCLOAK_REALMS=25

# Machines instance type
export INSTANCE_TYPE="m7i.4xlarge"

# Red Hat Pull Secret
export RHOCM_PULL_SECRET=''

#
# Script variables
# 
export WORKDIR=$(pwd)/workdir
